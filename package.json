  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./*": "./lib/*.js",
    "./components/*": "./lib/components/*.jsx",
    "./styles/*": "./lib/styles/*.css"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --platform=node --bundle --minify --outfile=build/index.js",
    "serve": "node build/index.js",
    "start": "nodemon src/index.ts",
    "test": "jest",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=\"-r dotenv/config\" DOTENV_CONFIG_PATH=.env.development npm run",
    "prod": "cross-env NODE_ENV=production NODE_OPTIONS=\"-r dotenv/config\" DOTENV_CONFIG_PATH=.env.production npm run"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.370.0",
    "@aws-sdk/types": "^3.370.0",
    "@jest/globals": "^29.6.1",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.17.15",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "zod": "^3.21.4"
  }
}
